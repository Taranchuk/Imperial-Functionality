<?xml version="1.0" encoding="utf-8"?>

<Defs>
	<RecipeDef Name="TSP_IF_VFED_USAMainstreamMedia_Propagandizing" Abstract="True">
		<!-- <defName>TSP_IF_VFED_USAMainstreamMedia_Propagandizing_Base</defName> -->
		<allowMixingIngredients>true</allowMixingIngredients>
		<dontShowIfAnyIngredientMissing>true</dontShowIfAnyIngredientMissing>
		<workSkill>Social</workSkill>
		<workSkillLearnFactor>4</workSkillLearnFactor>
		<soundWorking>Recipe_Tailor</soundWorking>
		<description>As a member of the Empire, you can please your liege by compiling Imperial Propaganda to spread to the unwashed masses. Unlike the demoralizing propaganda dropped to sow dissent among Deserters, this form of Imperial Propaganda is hand crafted by an august royal with the wholesome intent of winning over friends and allies.</description>
		<workSpeedStat>NegotiationAbility</workSpeedStat>
		<descriptionHyperlinks>
			<li>IF_ImperialPropaganda</li>
			<li>VFED_Filth_Propaganda</li>
		</descriptionHyperlinks>
		<jobString>Propagandizing</jobString>
		<unfinishedThingDef Inherit="False">TSP_IF_VFED_ImperialPropaganda_InProgress</unfinishedThingDef>
		<requiredGiverWorkType>Art</requiredGiverWorkType>
		<workSkill>Social</workSkill>
		<fixedIngredientFilter>
			<categories>
				<li>Textiles</li>
				<li MayRequire="mlie.rimwriter">RimWriter_BookMaterials</li>
			</categories>
			<thingDefs>
				<li>Cloth</li>
			</thingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>Textiles</li>
				<li MayRequire="mlie.rimwriter">RimWriter_BookMaterials</li>
			</categories>
			<thingDefs>
				<li>Cloth</li>
			</thingDefs>
		</defaultIngredientFilter>
		<recipeUsers Inherit="False">
			<li>CraftingSpot</li>
			<li>ElectricTailoringBench</li>
			<li>HandTailoringBench</li>
			<li MayRequire="vanillaexpanded.vbrewe">VBE_Bar</li>
			<li MayRequire="mlie.rimwriter">RimWriter_TableTypewriter</li>
			<li MayRequire="mlie.rimwriter">RimWriter_PapermakingBench</li>
			<li MayRequire="mlie.rimwriter">RimWriter_PapermakingBenchElectric</li>
		</recipeUsers>
		<researchPrerequisites Inherit="False">
			<li>IF_ImperialPropagandaTactics</li>
			<!-- <li MayRequire="mlie.rimwriter">RimWriter_TechTypewriting</li> -->
			<li MayRequire="mlie.rimwriter">RimWriter_BookMaterials</li>
		</researchPrerequisites>
	</RecipeDef>
	
